generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id         String   @id @default(uuid())
  ts         BigInt
  type       String
  slug       String?  // calculator slug
  page       String?  // page path for wizard
  sessionId  String?
  ua         String?
  payload    Json?
  createdAt  DateTime @default(now())

  @@index([ts])
  @@index([type])
}

